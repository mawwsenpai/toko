{
    "build": {
        "env": {
            "NODE_ENV": "production"
        }
    },
    "routes": [

        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },

        {
            "src": "/",
            "dest": "/home.html"
        },

        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}